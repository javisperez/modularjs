{
  "name": "<%= _.camelCase(appname) %>",
  "version": "0.0.1",
  "dependencies": {
    "jquery": "~2.*",
    "angular": "~1.4.*",
    "angular-menu": "*",
    "angular-animate": "~1.*",
    "angular-touch": "~1.*",
    "angular-cookies": "~1.*",
    "angular-sanitize": "~1.*",
    "angular-resource": "~1.*",
    "angular-route": "~1.*",
    <% if (bootstrap) { %>
    "bootstrap-sass-official": "~3.*",
    "angular-bootstrap": "*",
    "font-awesome": "~4.4.x",
    <% } else { %>
    "angular-material": "1.*",
    <% } %>
    "toastr": "*",
    "angular-toastr": "*",
    "sugar": "~1.4.1",
    "angular-loading-bar": "*"
  },
  "devDependencies": {
    "angular-mocks": "~1.*"
  },
  "resolutions": {
    "angular": "~1.4.*"
  },
  "overrides": {
      "font-awesome": {
        "main": [
          "./css/font-awesome.css",
          "./fonts/*"
        ]
      }
  }
}
